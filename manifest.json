{
  "manifest_version": 2,

  "name": "OpenCompare extension",
  "description": "This extension will replace any table by OpenCompare editor",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ]
}